# YouTube OAuth Credentials
# Get these from Google Cloud Console → APIs & Services → Credentials
YOUTUBE_CLIENT_ID=your_client_id_here
YOUTUBE_CLIENT_SECRET=your_client_secret_here

# YouTube Refresh Token
# Generated by running: python scripts/get_refresh_token.py
YOUTUBE_REFRESH_TOKEN=your_refresh_token_here

# YouTube Playlist IDs
# Find in playlist URL: https://www.youtube.com/playlist?list=PLAYLIST_ID
YOUTUBE_INPUT_PLAYLIST=your_input_playlist_id_here
YOUTUBE_OUTPUT_PLAYLIST=your_output_playlist_id_here

# Anthropic API Key
# Get from: https://console.anthropic.com/
ANTHROPIC_API_KEY=sk-ant-xxxxx

# Notion Integration
# Create at: https://www.notion.so/my-integrations
NOTION_API_KEY=secret_xxxxx

# Notion Database ID
# From database URL: https://notion.so/workspace/DATABASE_ID?v=...
NOTION_DATABASE_ID=your_database_id_here
