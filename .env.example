# YouTube OAuth credentials (from Google Cloud Console)
YOUTUBE_CLIENT_ID=your_client_id_here
YOUTUBE_CLIENT_SECRET=your_client_secret_here
YOUTUBE_REFRESH_TOKEN=your_refresh_token_here

# YouTube playlist IDs (from playlist URLs)
YOUTUBE_INPUT_PLAYLIST=PLxxxxxxxxxxxxxxxx
YOUTUBE_OUTPUT_PLAYLIST=PLyyyyyyyyyyyyyyyy

# Supadata API key (from supadata.ai)
SUPADATA_API_KEY=your_supadata_api_key_here

# Anthropic API key (from console.anthropic.com)
ANTHROPIC_API_KEY=sk-ant-xxxxxxxxxxxxx

# Notion credentials (from notion.so/my-integrations)
NOTION_API_KEY=secret_xxxxxxxxxxxxx
NOTION_DATABASE_ID=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
